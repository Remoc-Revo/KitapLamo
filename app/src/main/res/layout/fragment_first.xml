            <Button
                android:id="@+id/walo_kanisa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/white"
                android:text="16. Lamo mar Walo Kanisa"
                android:textAlignment="textStart"
                android:textAllCaps="false"
                android:textColor="@color/black" />

            <Button
                android:id="@+id/duoko_kanisa_e_tije_mamoko"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/white"
                android:text="17. Lamo ma Itimo ni Kanisa mane Osewal to koro Iduoko e Tije Mamoko"
